<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.giddu.musicalstructureapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"
            android:label="Home">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NowPlayingActivity"
            android:label="Now Playing"/>
        <activity android:name=".RadioActivity"
            android:label="Radio"/>
        <activity android:name=".PlaylistsActivity"
            android:label="Playlists"/>
        <activity android:name=".SongsActivity"
            android:label="Songs"/>
        <activity android:name=".ArtistsActivity"
            android:label="Artists"></activity>
    </application>

</manifest>